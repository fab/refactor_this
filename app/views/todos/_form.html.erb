<%= flash[:notice] if flash[:notice] %>
<%= form_for @todo do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title, :required => true %>
  <%= f.label :body %>
  <%= f.text_field :body, :required => true %>
  <%= f.label :list_name %>
  <%= f.text_field :list_name, :required => true %>
  <%= f.submit %>
<% end %>

